VER=2.0rc3
PVER=2.0-rc3
if [[ "${CROSS:-$ARCH}" = "amd64" ]]; then
	# Use staging
	SRCTBL="https://github.com/wine-compholio/wine-patched/archive/staging-$PVER.tar.gz"
	SUBDIR="wine-patched-staging-$PVER"
else
	# Use the original here
	SRCTBL="http://mirrors.ibiblio.org/wine/source/2.0/wine-$PVER.tar.bz2"
	SUBDIR="wine-$PVER"
fi
