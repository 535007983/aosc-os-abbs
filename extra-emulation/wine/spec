VER=2.0rc6
if [[ "${CROSS:-$ARCH}" = "amd64" ]]; then
	# Use staging
	SRCTBL="https://github.com/wine-compholio/wine-patched/archive/staging-${VER/rc/-rc}.tar.gz"
	SUBDIR="wine-patched-staging-${VER/rc/-rc}"
else
	# Use the original here
	SRCTBL="http://mirrors.ibiblio.org/wine/source/2.0/wine-${VER/rc/-rc}.tar.bz2"
	SUBDIR="wine-${VER/rc/-rc}"
fi
