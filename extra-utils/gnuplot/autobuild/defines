PKGNAME=gnuplot
PKGSEC=utils
PKGDEP="x11-lib readline cairo libgd"
PKGDES="plotting package which outputs to X11, PostScript, PNG, GIF and others"

AUTOTOOLS_AFTER="--with-gihdir=/usr/share/gnuplot --with-readline=gnu --disable-qt --libexecdir=/usr/bin --disable-wxwidgets"
ABMK="pkglibexecdir=/usr/bin"

ABSHADOW=no
NOPARALLEL=yes
AB_FLAGS_PIE=0
