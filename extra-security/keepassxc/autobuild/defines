PKGNAME=keepassxc
PKGSEC=security
PKGDES="a cross-platform community-driven port for Keepass"
PKGDEP="qt-5 libgcrypt zlib libmicrohttpd yubikey-personalization"

CMAKE_AFTER="-DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=/usr -DCMAKE_INSTALL_LIBDIR=lib -DWITH_XC_AUTOTYPE=ON -DWITH_XC_HTTP=ON -DWITH_XC_YUBIKEY=ON"
