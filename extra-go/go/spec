# Support for pre-POWER8 PPC64 targets removed by 1.9.
if [[ "${CROSS:-$ARCH}" != "ppc64" ]]; then
    VER=1.12.9
    CHKSUM="sha256::ab0e56ed9c4732a653ed22e232652709afbf573e710f56a07f7fdeca578d62fc"
else
    VER=1.8.7
    CHKSUM="sha256::5911e751807eebbc1980dad4305ef5492b96d6cd720bf93cbcefa86e1c195f9e"
fi
SRCTBL="https://dl.google.com/go/go$VER.src.tar.gz"
