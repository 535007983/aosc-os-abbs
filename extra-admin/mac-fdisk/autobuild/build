make

install -Dm755 pdisk "$PKGDIR"/usr/bin/pdisk
install -Dm755 fdisk "$PKGDIR"/usr/bin/mac-fdisk
ln -sv pdisk "$PKGDIR"/usr/bin/pmac-fdisk

install -Dm644 mac-fdisk.8.in "$PKGDIR"/usr/share/man/man8/mac-fdisk.8
install -Dm644 pmac-fdisk.8.in "$PKGDIR"/usr/share/man/man8/pdisk.8
ln -sv pdisk.8 "$PKGDIR"/usr/share/man/man8/pmac-fdisk.8
