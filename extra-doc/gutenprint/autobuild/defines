PKGNAME=gutenprint
PKGSEC=graphics
PKGDEP="foomatic cups ghostscript libusb"
BUILDDEP="gimp"
PKGDES="Top quality printer drivers for POSIX systems"

ABRPMAUTOPROVONLY=yes
AUTOTOOLS_AFTER="--disable-samples --enable-cups-ppds \
                 --enable-cups-ppds-at-top-level --enable-translated-cups-ppds \
                 --enable-globalized-cups-ppds --disable-static-genppd"
ABSHADOW=no
