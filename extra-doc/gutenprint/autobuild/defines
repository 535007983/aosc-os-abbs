                 --enable-escputil \
                 --enable-cups-1_2-enhancements \
                 --disable-cups-ppds \
                 --enable-simplified-cups-ppds \
                 --disable-foomatic \
                 --enable-ghostscript"
ABSHADOW=0

PKGBREAK="cinepaint<=1.0.4-7 gimp<=2.10.6-2"
