--stop-playback-on-init-failure=*)_mpv_s '--stop-playback-on-init-failure=yes --stop-playback-on-init-failure=no' "$cur"; return;;
--target-trc=*)_mpv_s '--target-trc=auto --target-trc=bt.1886 --target-trc=srgb --target-trc=linear --target-trc=gamma1.8 --target-trc=gamma2.2 --target-trc=gamma2.8 --target-trc=prophoto --target-trc=st2084 --target-trc=std-b67 --target-trc=v-log' "$cur"; return;;
