_mpv_s '--really-quiet= --rtsp-transport= --index= --resume-playback= --sub-clear-on-seek= --keepaspect= --vd-lavc-bitexact= --native-keyrepeat= --osd-bar= --sub-ass= --gapless-audio= --edition= --taskbar-progress= --stop-playback-on-init-failure= --target-trc= --ff-aid= --tscale-clamp= --vd-lavc-skiploopfilter= --osd-scale-by-window= --x11-bypass-compositor= --sub-align-x= --osd-italic= --sub-ass-scale-with-window= --vo-vaapi-scaled-osd= --input-right-alt-gr= --cscale-clamp= --dscale-clamp= --screen= --correct-downscaling= --screenshot-high-bit-depth= --demuxer-rawaudio-format= --oharddup= --jack-autostart= --msg-time= --sub-auto= --oautofps= --dvbin-full-transponder= --vo-tct-algo= --load-scripts= --write-filename-in-watch-later-config= --alpha= --icc-profile-auto= --vid= --vd-lavc-show-all= --demuxer-lavf-allow-mimetype= --vd-lavc-software-fallback= --hidpi-window-scale= --deband= --pause= --sid= --osd-align-x= --opengl-debug= --ad-lavc-downmix= --audio-pitch-correction= --oneverdrop= --vd-lavc-skipidct= --cdda-cdtext= --vo-image-tag-colorspace= --vd-lavc-fast= --scale-clamp= --force-window= --sub-align-y= --mute= --ovfirst= --sub-ass-force-margins= --opengl-waitvsync= --merge-files= --cache-pause= --vo-vaapi-scaling= --sub-gray= --linear-scaling= --vo-vdpau-chroma-deint= --demuxer-mkv-subtitle-preroll= --vd-lavc-framedrop= --cursor-autohide= --input-default-bindings= --ff-vid= --force-rgba-osd-rendering= --snap-window= --loop-file= --audio-fallback-to-null= --ao-pcm-append= --audio-file-auto= --alsa-resample= --input-test= --save-position-on-quit= --msg-module= --prefetch-playlist= --jack-connect= --interpolation= --sub-ass-justify= --field-dominance= --vo-vdpau-composite-detect= --sub-ass-style-override= --sub-ass-vsfilter-aspect-compat= --vo-image-high-bit-depth= --target-prim= --embeddedfonts= --sub-ass-vsfilter-blur-compat= --opengl-early-flush= --tv-forceaudio= --opengl-rectangle-textures= --vd-lavc-skipframe= --dither-depth= --demuxer-mkv-probe-start-time=