#!/bin/bash
# mpv 0.21.0
### LOOKUP TABLES AND CACHES ###
declare _mpv_xrandr_cache
declare -A _mpv_fargs
declare -A _mpv_pargs
_mpv_fargs[vo@xv]="adaptor= port= buffers= ck= colorkey= ck-method="
_mpv_pargs[vo@xv@adaptor]="-1"
_mpv_pargs[vo@xv@port]="0"
_mpv_pargs[vo@xv@buffers]="2 1 10"
_mpv_pargs[vo@xv@ck]="cur use set"
_mpv_pargs[vo@xv@colorkey]="65280"
_mpv_pargs[vo@xv@ck-method]="man none bg auto"
_mpv_fargs[vo@vdpau]="sharpen= queuetime-windowed= colorkey= pullup= hqscaling= output-surfaces= fps= force-yuv= composite-detect= queuetime-fs= chroma-deint= deint= denoise="
_mpv_pargs[vo@vdpau@sharpen]="0 -1 1"
_mpv_pargs[vo@vdpau@queuetime-windowed]="50"
_mpv_pargs[vo@vdpau@colorkey]="#ffffff 1.0/1.0/1.0/1.0"
_mpv_pargs[vo@vdpau@pullup]="yes no"
_mpv_pargs[vo@vdpau@hqscaling]="0 9"
_mpv_pargs[vo@vdpau@output-surfaces]="3 2 15"
_mpv_pargs[vo@vdpau@fps]="0"
_mpv_pargs[vo@vdpau@force-yuv]="yes no"
_mpv_pargs[vo@vdpau@composite-detect]="yes no"
_mpv_pargs[vo@vdpau@queuetime-fs]="50"
_mpv_pargs[vo@vdpau@chroma-deint]="yes no"
_mpv_pargs[vo@vdpau@deint]="0 -4 4"
_mpv_pargs[vo@vdpau@denoise]="0 1"
_mpv_fargs[vo@wayland]="alpha= rgb565="
_mpv_pargs[vo@wayland@alpha]="yes no"
_mpv_pargs[vo@wayland@rgb565]="yes no"
_mpv_fargs[vo@opengl]="es= tscale-param2= dither-depth= tone-mapping-param= lscale-down= deband-grain= backend= rectangle-textures= debug= tscale-clamp= cparam1= scaler-resizes-only= smoothmotion= pbo= tscale-blur= dither-size-fruit= scale= dwmflush= deband-iterations= correct-downscaling= blend-subtitles= cscale-param2= dscale-antiring= cscale-antiring= dscale-blur= waitvsync= deband-range= cscale-blur= user-shaders= scale-antiring= gamma= lradius= fancy-downscaling= scale-down= smoothmotion-threshold= deband= cantiring= gamma-auto= sharpen= icc-profile-auto= target-trc= lantiring= lparam2= scale-radius= scale-param1= lparam1= lscale= temporal-dither-period= icc-contrast= icc-intent= sigmoid-center= background= interpolation-threshold= cradius= scale-clamp= scale-wparam= icc-profile= vsync-fences= deband-threshold= icc-cache-dir= temporal-dither= hdr-tone-mapping= cscale-param1= alpha= 3dlut-size= tscale= fbo-format= dscale-window= cscale-window= tscale-wparam= dscale-radius= cscale-radius= cscale= sigmoid-slope= glfinish= dscale= sigmoid-upscaling= interpolation= linear-scaling= tscale-param1= scale-window= cparam2= tscale-antiring= tscale-window= target-prim= dscale-param1= tscale-radius= dither= check-pattern= cscale-clamp= cscale-wparam= sw= target-brightness= scaler-lut-size= scale-param2= dscale-param2= dscale-clamp= dscale-wparam= dumb-mode= dcomposition= swapinterval= scale-blur="
_mpv_fargs[vo@vaapi]="scaled-osd= scaling="
_mpv_pargs[vo@vaapi@scaled-osd]="yes no"
_mpv_pargs[vo@vaapi@scaling]="default fast hq nla"
_mpv_fargs[vo@image]="high-bit-depth= jpeg-smooth= png-filter= outdir= jpeg-quality= jpeg-source-chroma= tag-colorspace= format= png-compression="
_mpv_fargs[vo@null]="fps="
_mpv_pargs[vo@null@fps]="0 10000"
_mpv_fargs[ao@pcm]="append= file= waveheader="
_mpv_pargs[ao@pcm@append]="yes no"
_mpv_pargs[ao@pcm@waveheader]="yes no"
_mpv_fargs[ao@oss]="mixer-channel= mixer-device="
_mpv_pargs[ao@oss@mixer-device]="/dev/mixer"
_mpv_fargs[ao@null]="untimed= buffer= outburst= channel-layouts= speed= broken-delay= broken-eof= latency="
_mpv_pargs[ao@null@untimed]="yes no"
_mpv_pargs[ao@null@buffer]="0.2 0 100"
_mpv_pargs[ao@null@outburst]="256 1 100000"
_mpv_pargs[ao@null@speed]="1 0 10000"
_mpv_pargs[ao@null@broken-delay]="yes no"
_mpv_pargs[ao@null@broken-eof]="yes no"
_mpv_pargs[ao@null@latency]="0 100"
