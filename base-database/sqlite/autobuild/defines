PKGNAME=sqlite
PKGDES="SQLite software library"
PKGDEP="readline tcl"
PKGSEC=database

NOPARALLEL=yes

AUTOTOOLS_AFTER="--disable-amalgamation \
                 --enable-fts3 \
                 --enable-fts4 \
                 --enable-fts5 \
                 --enable-rtree \
                 --enable-json1 \
		 --enable-update-limit \
		 --enable-geopoly \
		 --enable-session"
ABSHADOW=no
NOPARALLEL=yes
