sh "$SRCDIR"/build.sh

gcc core/mkbootimg/mkbootimg.c core/libmincrypt/sha.c -Icore/include -o mkbootimg
gcc core/mkbootimg/unpackbootimg.c core/libmincrypt/sha.c -Icore/include -o unpackbootimg

install -m755 -d "$PKGDIR"/usr/bin
install -m755 -t "$PKGDIR"/usr/bin mkbootimg unpackbootimg fastboot adb
